{
  "name": "reactnode",
  "private": true,
  "version": "1.0.0",
  "description": "node react",
  "main": "servermodule.js",
  "type": "module",
  "exports": {
    "import": "./servermodule.mjs",
    "require": "./index.cjs"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "devv": "nodemon watchvite.mjs",
    "dev:nodemon": "nodemon servermodule.mjs",
    "dev:webpack": "webpack -c webpack.config.cjs --watch",
    "vitebuild": "npm-run-all --parallel build:vite",
    "build:vite": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.jsx --outDir dist/server",
    "generate": "vite build --outDir dist/static && npm run build:server && node prerender.mjs",
    "start": "node servermodule.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "@vitejs/plugin-react": "^1.2.0",
    "axios": "^0.26.0",
    "body-parser": "^1.19.2",
    "chalk": "^5.0.0",
    "classnames": "^2.3.1",
    "compression": "^1.7.4",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.10.8",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "formidable": "^3.2.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.4",
    "nanoid": "^3.3.1",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2",
    "serve-static": "^1.14.2",
    "vite": "^2.8.6"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/runtime": "^7.17.2",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.6.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^3.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "nodemonConfig": {
    "ignore": [
      "/src/components/*",
      "docs/*"
    ]
  }
}
